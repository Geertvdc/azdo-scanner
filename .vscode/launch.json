    "version": "0.2.0",
    "configurations": [
        {
            "name": "C#: azdo-scanner Debug",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/src/azdo-scanner//bin/Debug/net9.0/azdo-scanner.dll",
            "args": [
                "list-projects",
                "--org",
                "geertvdc",
                "--projects",
                "Devops Demo, test-project-1",
                "--include-repos",
                "--include-serviceconnections"
            ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        }
    ]
}